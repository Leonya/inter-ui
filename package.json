  "repository": "git@github.com:philipbelesky/inter-ui.git",
  "private": false,
  "devDependencies": {
    "glyphhanger": "^5.0.0",
    "sass": "1.69.5"
  },
  "scripts": {
    "generate-css": "sass --no-source-map inter.scss:inter.css inter-hinted.scss:inter-hinted.css inter-latin.scss:inter-latin.css inter-hinted-latin.scss:inter-hinted-latin.css",
    "build-supersets": "npm run delete-supersets && npm run copy-supersets",
    "delete-supersets": "rm -fr './web' && mkdir './web' && rm -fr './variable' && mkdir './variable' && rm -fr './display' && mkdir './display'",
    "copy-supersets": "cp -a ./temp/InterVariable*.woff2 ./variable/ & cp -a ./temp/InterDisplay-*.woff2 ./display & cp -a ./temp/Inter-*.woff2 ./web",
    "build-subsets": "npm run delete-subsets && npm run subset-web && npm run subset-display && npm run subset-variable",
    "delete-subsets": "rm -fr './web-latin' && mkdir './web-latin' && rm -fr './variable-latin' && mkdir './variable-latin' && rm -fr './display-latin' && mkdir './display-latin'",
    "subset-web": "glyphhanger --subset='./temp/Inter-*.woff2' --LATIN --formats=woff2,woff && cp -a ./temp/Inter-*-subset.woff2 ./web-latin/",
    "subset-display": "glyphhanger --subset='./temp/InterDisplay-*.woff2' --LATIN --formats=woff2,woff && cp -a ./temp/InterDisplay-*-subset.woff2 ./display-latin/",
    "subset-variable": "glyphhanger --subset='./temp/InterVariable*.woff2' --LATIN --formats=woff2,woff && cp -a ./temp/InterVariable-*-subset.woff2 ./variable-latin/"
  }
}
