{
  "name": "inter-ui",
  "version": "3.19.3",
  "description": "Font files for the Inter font family.",
  "main": "inter.css",
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "inter",
    "inter-ui",
    "typeface",
    "font",
    "font family"
  ],
  "author": "Rasmus Andersson (https://rsms.me)",
  "contributors": [
